{
  "name": "camel",
    "description": "The Apache camel project, tweets, wiki, github, et. al",
  "label": "Apache Camel",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "camel-website",
      "label": "Apache camel Website",
      "url": "https://camel.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "camel-users",
      "label": "Camel Users",
      "mbox": "camel-users",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "camel-dev",
      "label": "Camel Dev",
      "mbox": "camel-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-camel",
    "label": "Apache Camel JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/camel",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
